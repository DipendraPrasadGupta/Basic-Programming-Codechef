#include <stdio.h>

int main(void) {
	int t;
	scanf("%d",&t);
	for( int i=0; i<t; i++)
	{
	    int a,b;
	    scanf("%d%d",&a,&b);
	    if(a>b)
	    {
	        printf("LOSS\n");
	    }
	    else if(a<b)
	    {
	        printf("PROFIT\n");
	    }
	    else
	    {
	        printf("NEUTRAL\n");
	    }
	}
	return 0;
}

